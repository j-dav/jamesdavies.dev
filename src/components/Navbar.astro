---
const links = [
  { href: "/work", text: "work" },
  { href: "/projects", text: "projects" },
  { href: "/blog", text: "blog" },
];
---

<div class="container">
  <div class="navbar">
    <h2><a href="https://jamesdavies.dev">jamesdavies</a></h2>
    <nav>
      <ul>
        {
          links.map((link) => (
            <li>
              <a href={link.href}>{link.text}</a>
            </li>
          ))
        }
      </ul>
    </nav>
  </div>
</div>
